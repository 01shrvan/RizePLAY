{
  "expo": {
    "fonts": [
      {
        "asset": "./assets/fonts/SpaceMono-Regular.ttf",
        "family": "SpaceMono-Regular"
      },
      {
        "asset": "./assets/fonts/SpaceMono-Medium.ttf",
        "family": "SpaceMono-Medium"
      },
      {
        "asset": "./assets/fonts/SpaceMono-Bold.ttf",
        "family": "SpaceMono-Bold"
      }
    ],
    "name": "rizeplay",
    "slug": "rizeplay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rizeplay",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
